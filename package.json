{
  "name": "typegoose",
  "version": "5.2.1",
  "description": "Define Mongoose models using TypeScript classes.",
  "main": "lib/typegoose.js",
  "scripts": {
    "start": "npm run build && node ./lib/typegoose.js",
    "build": "rimraf lib && tsc",
    "lint": "tslint --project tsconfig.json",
    "test": "npm run lint && jest --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/szokodiakos/typegoose.git"
  },
  "types": "lib/typegoose.d.ts",
  "typings": "lib/typegoose.d.ts",
  "author": "Akos Szokodi <akos@codingsans.com> (http://codingsans.com)",
  "license": "MIT",
  "jest": {
    "verbose": true,
    "automock": false,
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "globals": {
      "ts-jest": {
        "enableTsDiagnostics": true,
        "tsConfigFile": "./__tests__/tsconfig.json"
      }
    },
    "transform": {
      "^.+\\.(ts$)": "ts-jest"
    },
    "setupTestFrameworkScriptFile": "./__tests__/setup.ts",
    "testRegex": "(__tests__/.*(spec))\\.(ts)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "__tests__/(.*)": "<rootDir>/__tests__/$1",
      "src/(.*)": "<rootDir>/src/$1"
    }
  },
  "dependencies": {
    "@types/mongoose": "5.0.10",
    "lodash": "4.17.10",
    "mongoose-schema-extend": "^0.2.2"
  },
  "peerDependencies": {
    "mongoose": "^5.0.0",
    "reflect-metadata": "^0.1.8"
  },
  "devDependencies": {
    "mongoose": "^5.0.0",
    "reflect-metadata": "^0.1.8",
    "@types/dotenv": "4.0.3",
    "@types/jest": "^22.2.3",
    "@types/lodash": "4.14.108",
    "@types/node": "8.10.0",
    "chai": "4.1.2",
    "coveralls": "3.0.0",
    "dotenv": "5.0.1",
    "istanbul": "0.4.5",
    "jest": "^22.4.3",
    "jest-extended": "^0.7.1",
    "mocha": "3.2.0",
    "mongo-interlude": "^0.2.1",
    "mongoose-findorcreate": "3.0.0",
    "rimraf": "2.6.2",
    "ts-jest": "^22.4.6",
    "tslint": "5.9.1",
    "typescript": "2.8.3"
  }
}
